					<uxap:ObjectPageHeaderActionButton type="Transparent" text="Copy" icon="sap-icon://copy"/>
					<uxap:ObjectPageHeaderActionButton type="Transparent" text="Add" icon="sap-icon://add"/>
					<uxap:ObjectPageHeaderActionButton icon="sap-icon://action" type="Transparent" text="Share"/>
				</uxap:actions>
				<uxap:navigationActions>
					<uxap:ObjectPageHeaderActionButton icon="sap-icon://slim-arrow-up" type="Transparent"/>
					<uxap:ObjectPageHeaderActionButton icon="sap-icon://slim-arrow-down" type="Transparent"/>
				</uxap:navigationActions>
				<uxap:content>
					<m:GenericTag text="Material Shortage" status="Warning"/>
				</uxap:content>
			</uxap:ObjectPageDynamicHeaderTitle>
		</uxap:headerTitle>
		<uxap:headerContent>
			<l:HorizontalLayout allowWrapping="true">
